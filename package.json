{
  "name": "strapi-plugin-navigation",
  "version": "3.0.0-beta.0",
  "description": "Strapi - Navigation plugin",
  "strapi": {
    "name": "navigation",
    "displayName": "Navigation",
    "description": "Create consumable navigation with a simple and straightforward visual builder",
    "kind": "plugin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VirtusLab/strapi-plugin-navigation"
  },
  "scripts": {
    "publish:latest": "cd build && npm publish --tag latest",
    "publish:beta": "cd build && npm publish --tag beta",
    "prepublish": "yarn clean && yarn build && ts-node ./setup-package.ts",
    "test:unit": "jest --verbose --coverage",
    "test:unit:watch": "jest --watch",
    "build": "tsc -p ./server/tsconfig.json && tsc -p ./admin/tsconfig.json",
    "build:dev": "npm run build && cp ./package.json ./build && cd ./build && yarn",
    "build:admin:watch": "tsc -p ./admin/tsconfig.json --watch",
    "build:server:watch": "tsc -p ./server/tsconfig.json --watch",
    "clean": "rm -rf build",
    "develop": "nodemon --exec \"npm run build:dev\""
  },
  "types": "./types/index.d.ts",
  "dependencies": {
    "@hookform/resolvers": "3.4.2",
    "@paralleldrive/cuid2": "2.2.2",
    "@sindresorhus/slugify": "1.1.0",
    "@tanstack/react-query": "5.37.1",
    "lodash": "^4.17.21",
    "pluralize": "8.0.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-intl": "6.6.8",
    "react-router-dom": "^6.22.3",
    "uuid": "^10.0.0",
    "zod": "3.22.5"
  },
  "devDependencies": {
    "@jest/types": "29.5.x",
    "@koa/router": "^12.0.1",
    "@strapi/design-system": "2.0.0-rc.10",
    "@strapi/icons": "2.0.0-rc.10",
    "@strapi/plugin-graphql": "5.0.0-rc.22",
    "@strapi/strapi": "5.0.0-rc.22",
    "@strapi/typescript-utils": "4.22.1",
    "@types/jest": "29.5.1",
    "@types/koa": "2.15.0",
    "@types/koa__router": "^12.0.4",
    "@types/lodash": "^4.14.181",
    "@types/node": "^20.12.0",
