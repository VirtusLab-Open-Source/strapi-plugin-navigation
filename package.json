    "generate-lockfile": "0.0.12",
    "jest": "29.6.0",
    "jest-cli": "^27.5.1",
    "jest-styled-components": "^7.0.2",
    "koa": "^2.15.3",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.15",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.5",
    "react-intl": "^6.6.8",
    "react-router-dom": "^6.22.3",
    "strapi-plugin-rest-cache": "^4.2.9",
    "styled-components": "^6.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "^10.7.0",
    "typescript": "5.4.4"
  },
  "peerDependencies": {
